{
    "name": "webklex/translator",
    "type": "library",
    "description": "Flexible Laravel translator based on .CSV language files",
    "keywords": [
        "webklex",
        "laravel",
        "language",
        "multilang",
        "csv",
        "translator"
    ],
    "homepage": "https://github.com/webklex/translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Malte Goldenbaum",
            "email": "github@webklex.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": ">=5.2",
        "illuminate/contracts": ">=5.2",
        "illuminate/config": ">=5.2",
        "illuminate/translation": ">=5.2",
        "illuminate/support": ">=5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Webklex\\Translator\\": "src/Translator"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webklex\\Translator\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
